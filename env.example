######## Server ########
HTTP_PORT=8080
GRPC_PORT=50051
LOG_LEVEL=info
JWT_SECRET=change-me-in-prod
AAA_JWT_SECRET=
AAA_ENABLE_DOCS=true
# AAA_RUN_SEED controls database seeding
# Values: "false" (no database seeding), "true" (seed database with initial data)
# Note: PostgreSQL RBAC schema is seeded when this setting is enabled
AAA_RUN_SEED=false
AAA_AUTO_MIGRATE=false
AAA_CORS_ALLOWED_ORIGINS=

# Database Configuration
DB_PRIMARY_BACKEND=gorm
DB_POSTGRES_HOST=localhost
DB_POSTGRES_PORT=5432
DB_POSTGRES_USER=postgres
DB_POSTGRES_PASSWORD=your_password
DB_POSTGRES_DBNAME=kisanlink
DB_POSTGRES_SSLMODE=disable
DB_POSTGRES_MAX_CONNS=10
DB_POSTGRES_IDLE_CONNS=5
DB_POSTGRES_READ_REPLICAS=

# Note: PostgreSQL RBAC is used for authorization - no external authorization service needed

# DynamoDB Configuration (optional)
DB_DYNAMO_REGION=us-east-1
DB_DYNAMO_TABLE=

# S3 Configuration (optional)
DB_S3_REGION=us-east-1
DB_S3_BUCKET=

# Logging
DB_LOG_LEVEL=info

######## Redis ########
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
# Set CACHE_DISABLED=true to disable Redis caching (useful for local development without Redis)
CACHE_DISABLED=false

######## Database URL (for GORM) ########
# This is used by the SqlDB connection
DATABASE_URL=postgres://aaa_user:aaa_password@localhost:5432/aaa_service?sslmode=disable
